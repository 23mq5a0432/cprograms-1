#include<stdio.h>
void main()
{
    int i,n,count;
    printf("enter the n value:");
    scanf("%d",&n);
    count=0;
    for(i=1;i<=n;++i)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    if(count==2)
    {
        printf("it is an prime number");
    }
    else
    {
        printf("it is not a prime number");
    }
}
